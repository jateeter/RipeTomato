# Development Environment Configuration for CORS Proxy Server
# Copy to .env.proxy and customize for your environment

# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://localhost:3000,http://127.0.0.1:3000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=info
LOG_REQUESTS=true

# Security
TRUST_PROXY=false

# Health Check Configuration
HEALTH_CHECK_TIMEOUT=10000
HMIS_HEALTH_CHECK_ENABLED=true

# Proxy Timeouts
PROXY_TIMEOUT=30000
PROXY_RETRY_ATTEMPTS=3